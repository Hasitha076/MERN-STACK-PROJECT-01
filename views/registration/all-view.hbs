<div class="d-flex justify-content-between flex-wrap flex-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
  <h1 class="h2">{{data.module_name}} Registered Student's Info.</h1>
</div>

<div class="col py-3">

  <div class="row">
    <div class="col">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/module">Program Management</a></li>
          <li class="breadcrumb-item active">{{data.module_name}} Registered students Info.</li>
        </ol>
      </nav>
    </div>
  </div>

</div>

<div class="row">
{{#each rows}}
     <div class="col-4 mb-3 text-center">
    <div class="card">
      <div class="card-body">
        <i class="bi bi-person-check" style="font-size: 40px;"></i>
        <h5 class="card-title">{{this.student_name}}</h5>
      </div>
    </div>
  </div>
{{/each}}
</div>
